{
  "name": "twistersfury/phalcon-shared",
  "description": "Phalcon Shared Module - Adds Some Common/Shared Utility Classes To Work More Effectively With Phalcon",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "phalcon",
    "shared",
    "php",
    "utilities",
    "helpers",
    "crud"
  ],
  "authors": [
    {
      "name": "Twister's Fury",
      "email": "info@twistersfury.com"
    }
  ],
  "require" : {
    "php": "^7.1",
    "ext-phalcon": "^3.4.0",
    "monolog/monolog": "2.0.x-dev|^1.6"
  },
  "require-dev": {
    "codeception/codeception": "^2.3.3",
    "phalcon/ide-stubs": "^3.4.0",
    "mikey179/vfsStream": "v1.6.4",
    "phalcon/incubator": "3.4.x",
    "vlucas/phpdotenv": "^2.4"
  },
  "suggest": {
    "rollbar/rollbar" : "Required for the RollBar Service Provider"
  },
  "autoload-dev": {
    "psr-4": {
      "TwistersFury\\Phalcon\\Shared\\Tests\\Mocks\\" : "tests/_support/Mocks/",
      "TwistersFury\\Phalcon\\Shared\\Tests\\Unit\\" : "tests/unit/"
    },
    "files" : [
      "local_bootstrap.php"
    ]
  }
}
